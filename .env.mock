# KIS Estimator Mock Environment Configuration
APP_ENV=test
ENVIRONMENT=mock

# Mock JWT Configuration
JWT_AUD=authenticated
SUPABASE_JWT_SECRET=mock-secret-key-for-testing-only-not-for-production
JWT_SECRET=mock-secret-key-for-testing-only-not-for-production
JWT_ISSUER=kis-estimator-mock
JWT_ALGORITHM=HS256

# Mock Database (SQLite in-memory)
DATABASE_URL=sqlite:///:memory:
DB_POOL_SIZE=5
DB_POOL_MAX_OVERFLOW=10

# Mock Redis
REDIS_URL=memory://
CACHE_TTL=300

# Mock Supabase
SUPABASE_URL=http://localhost:9999
SUPABASE_ANON_KEY=mock-anon-key
SUPABASE_SERVICE_ROLE_KEY=mock-service-key

# Security Settings
CORS_ORIGINS=http://localhost:3000,http://localhost:8000
TRUSTED_HOSTS=localhost,127.0.0.1
RATE_LIMIT_PER_MINUTE=100

# Performance Settings
ASYNC_WORKERS=4
CONNECTION_TIMEOUT=15
READ_TIMEOUT=30
POOL_RECYCLE=300

# Evidence & Logging
EVIDENCE_PATH=/tmp/kis-estimator-mock/evidence
LOG_LEVEL=DEBUG
ENABLE_TRACING=true